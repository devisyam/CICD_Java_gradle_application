pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'build is completed'
            }
        }
        
        stage('Test') {
            steps {
                sh 'testing is successfully'
            }
        }
        
        stage('Deploy') {
            steps {
                sh 'Deployment done successfully'
            }
        }
    }
}
